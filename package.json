{
  "name": "zaantrans",
  "version": "0.1.1",
  "private": true,
  "prisma": {
    "seed": "ts-node -T prisma/seed.ts"
  },
  "scripts": {
    "postinstall": "husky install",
    "dev": "next dev -p 3400",
    "build": "prisma generate && next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "gen": "fx add resource",
    "prisma:migrate:dev": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:prod": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate deploy",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:seed:dev": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma db seed",
    "prisma:seed:staging": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.staging.local -- prisma db seed",
    "prisma:seed:prod": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.production.local -- prisma db seed",
    "prisma:reset": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate reset",
    "prisma:studio": "prisma studio",
    "docker:stop": "docker-compose down",
    "heroku-postbuild": "next build && npm run prisma:migrate:prod"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "@chakra-ui/theme-tools": "^2.0.16",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@faker-js/faker": "^7.6.0",
    "@fontsource/nunito": "^5.1.0",
    "@prisma/client": "^5.21.1",
    "@quixo3/prisma-session-store": "^3.1.10",
    "aws-sdk": "^2.1333.0",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "chakra-dayzed-datepicker": "^0.2.7",
    "chakra-react-select": "^4.6.0",
    "chalk": "^5.2.0",
    "cookie-signature": "^1.2.1",
    "cross-fetch": "^3.1.5",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.29.3",
    "dayzed": "^3.2.3",
    "dotenv-cli": "^7.1.0",
    "framer-motion": "^10.2.4",
    "generate-colors": "^1.4.2",
    "http": "^0.0.1-security",
    "human-readable-numbers": "^0.9.5",
    "lodash": "^4.17.21",
    "next": "^13.2.4",
    "next-connect": "^0.13.0",
    "next-session": "^4.0.5",
    "next-translate": "^2.0.2",
    "next-translate-plugin": "^2.0.2",
    "passport": "^0.5.3",
    "passport-local": "^1.0.0",
    "posthog-js": "^1.50.8",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.1",
    "react-icons": "^4.8.0",
    "react-moment": "^1.1.3",
    "react-query": "^3.39.3",
    "react-timeago": "^7.1.0",
    "react-window": "^1.8.8",
    "ts-custom-error": "^3.3.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@founding/fx": "latest",
    "@svgr/cli": "^6.5.1",
    "@svgr/webpack": "^6.5.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-signature": "^1.1.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.0",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-query": "^1.2.8",
    "@types/react-timeago": "^4.1.3",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "prisma": "^5.21.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "20.x"
  }
}
